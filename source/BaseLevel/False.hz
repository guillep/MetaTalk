False
	superclass: #Boolean;

False >> initialize [
]

False >> | aBoolean [
	^ aBoolean
]

False >> & aBoolean [
	^ false
]

False >> not [
	^ true
]

False >> ifTrue: aBlock [
	^ nil
]

False >> ifFalse: aBlock [
	^ aBlock value
]

False >> ifTrue: aBlock ifFalse: anotherBlock [
	^ anotherBlock value
]

False >> asString [
	^ 'false'
]