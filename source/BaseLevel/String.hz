String
    superclass: #Object;
    instanceSpecification: #(#variable #byte ).


String >> at: index [
    <primitive: 63>
]

String >> at: index put: aChar [
    <primitive: 64>
]

String >> byteAt: index put: aByte [
    <primitive: 61>
]

String >> size [
    <primitive: 62>
]

String >> print [
    | f |
    f := File new.
    f openReadWrite: './log.txt'.
    f position: f size.
    f nextPutAll: self.
    f cr.
    f close
]

String >> asString [
    ^ self
]