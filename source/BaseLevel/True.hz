True
	superclass: #Boolean;

True >> initialize [
]

True >> | aBoolean [
	^ true
]

True >> & aBoolean [
	^ aBoolean
]

True >> not [
	^ false
]

True >> ifTrue: aBlock [
	^ aBlock value
]

True >> ifFalse: aBlock [
	^ nil
]

True >> ifTrue: aBlock ifFalse: anotherBlock [
	^ aBlock value
]

True >> asString [
	^ 'true'
]